{
    "compilerOptions": {
        "emitDecoratorMetadata": true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "jsx": "react-jsx",
        "moduleResolution": "node",
        "noUncheckedIndexedAccess": true,
        "noUnusedLocals": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "strict": true,
        "strictNullChecks": true
    }
}
