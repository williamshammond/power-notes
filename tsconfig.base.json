{
    "compilerOptions": {
        "strict": true,
        "strictNullChecks": true,
        "esModuleInterop": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "noUnusedLocals": true,
        "noUncheckedIndexedAccess": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "jsx": "react-jsx",
        "moduleResolution": "node"
    }
}
